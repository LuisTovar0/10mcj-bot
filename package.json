    "@types/uuid": "^8.3.4",
    "axios": "^1.0.0",
    "botgram": "^2.2.0",
    "del": "^6.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "moment": "^2.29.4",
    "mongoose": "^6.6.2",
    "mp3-duration": "^1.1.0",
    "newcleanfolder": "2.4.0",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.7.0",
    "typedi": "^0.10.0",
    "typescript": "^4.8.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/randomstring": "^1.1.8",
    "@types/sinon": "^10.0.13",
