{
  "name": "10mcj-bot",
  "engines": {
    "node": ">=16 <20"
  },
  "scripts": {
    "start": "node build/app.js",
    "ts-start": "ts-node ./src/app.ts",
    "dev": "concurrently \"nodemon build/app.js\" \"tsc -w\"",
    "ts-dev": "nodemon --exec \"node --loader ts-node/esm\"  ./src/app.ts",
    "build": "tsc",
    "prestart": "npm run build",
    "test": "env-cmd -f .test.env mocha -r ts-node/register test/**/*.test.ts",
    "test-repos": "env-cmd -f .test.env mocha -r ts-node/register test/**/repo/**/*.test.ts",
    "test-services": "env-cmd -f .test.env mocha -r ts-node/register test/**/service/**/*.test.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/uuid": "^9.0.1",
    "axios": "^1.4.0",
    "botgram": "^2.2.0",
    "canvas": "^2.11.2",
    "del": "^7.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "lorem-ipsum": "^2.0.8",
    "moment": "^2.29.4",
    "mongoose": "^7.1.1",
    "mp3-duration": "^1.1.0",
    "newcleanfolder": "^2.4.0",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typedi": "^0.10.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/randomstring": "^1.1.8",
    "@types/sinon": "^10.0.13",
    "concurrently": "^8.0.1",
    "env-cmd": "^10.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "sinon": "^15.0.4"
  }
}
